    "vue-chartjs": "^5.2.0",
    "vue-router": "4.2.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.5",
    "@types/node": "^16.18.23",
    "@vitejs/plugin-vue": "^4.2.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.42.0",
